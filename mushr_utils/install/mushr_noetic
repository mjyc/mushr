export MUSHR_INSTALL_PATH=/home/vscode/catkin_ws/src/mushr/mushr_utils/install
export MUSHR_REAL_ROBOT=0
export MUSHR_WS_PATH=/home/vscode
export MUSHR_COMPOSE_FILE=docker-compose-cpu.yml
export MUSHR_OS_TYPE=x86_64
if [ $# == 0 ] || [ $1 == "run" ];
then
  if [ 1 == 2 ];
  then
    docker-compose -f $MUSHR_INSTALL_PATH/$MUSHR_COMPOSE_FILE run -p 	9090:9090 mushr_noetic bash -ic "${2}"
  else
    docker-compose -f $MUSHR_INSTALL_PATH/$MUSHR_COMPOSE_FILE run -p 	9090:9090 mushr_noetic bash
  fi
elif [ $1 == "build" ];
then
  docker-compose -f /home/vscode/catkin_ws/src/mushr/mushr_utils/install/docker-compose-cpu.yml build --no-cache mushr_noetic
else
  echo "Invalid command supplied to mushr_noetic script; valid commands are 'run' or 'build'"
fi
